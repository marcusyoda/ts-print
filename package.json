{
  "name": "ts-print",
  "description": "Biblioteca útil para debug em node, com cores e datas formatadas, se propõe a ser intuitiva.",
  "version": "1.1.0",
  "license": "MIT",
  "author": "Marcus Yoda <marcusyoda13@gmail.com>",
  "repository": "https://github.com/marcusyoda/ts-print.git",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "files": [
    "lib",
    "package.json"
  ],
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "deploy": "yarn build && yarn publish",
    "lint": "./node_modules/.bin/eslint src/*.ts index.ts",
    "test": "mocha -r ts-node/register tests/*.test.ts"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chai": "^4.3.4",
    "eslint": "8.7.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "rollup": "^2.64.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}
